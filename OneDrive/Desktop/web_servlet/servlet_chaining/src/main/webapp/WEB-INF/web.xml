<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>servlet_chaining</display-name>
  <servlet>
  	<servlet-name>Login</servlet-name>
  	<servlet-class>servlet_chaining.Login_Servlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>Login</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>signup</servlet-name>
  	<servlet-class>servlet_chaining.SignUp_Servlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>signup</servlet-name>
  	<url-pattern>/signup</url-pattern>
  </servlet-mapping>
  
  
  <servlet>
  	<servlet-name>Login_1</servlet-name>
  	<servlet-class>send_redirect.SendRedirectMethod</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>Login_1</servlet-name>
  	<url-pattern>/login_1</url-pattern>
  </servlet-mapping>
  
</web-app>